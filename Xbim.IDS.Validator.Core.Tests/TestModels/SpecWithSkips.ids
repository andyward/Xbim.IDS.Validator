<?xml version="1.0" encoding="utf-8"?>
<ids:ids xmlns:xs="http://www.w3.org/2001/XMLSchema"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://standards.buildingsmart.org/IDS http://standards.buildingsmart.org/IDS/1.0/ids.xsd"
xmlns:ids="http://standards.buildingsmart.org/IDS">
  <ids:info>
    <ids:title>Spec that won't apply to anything</ids:title>
    <ids:author>andy.ward@xbim.net</ids:author>
  </ids:info>
  <ids:specifications>
    <ids:specification ifcVersion="IFC2X3 IFC4" name="If there are SPECIAL Chillers they should be named CHILL">
      <ids:applicability minOccurs="0" maxOccurs="unbounded">
        <ids:entity>
          <ids:name>
            <ids:simpleValue>IFCCHILLERTYPE</ids:simpleValue>
          </ids:name>
          <ids:predefinedType>
            <ids:simpleValue>SPECIAL</ids:simpleValue>
          </ids:predefinedType>
        </ids:entity>
      </ids:applicability>
      <requirements>
        <attribute cardinality="required">
          <name>
            <simpleValue>Name</simpleValue>
          </name>
          <value>
            <simpleValue>CHILL</simpleValue>
          </value>
        </attribute>
      </requirements>
    </ids:specification>
  </ids:specifications>
</ids:ids>