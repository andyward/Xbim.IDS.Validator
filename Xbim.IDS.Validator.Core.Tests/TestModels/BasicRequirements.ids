<?xml version="1.0" encoding="utf-8"?>
<ids:ids xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://standards.buildingsmart.org/IDS http://standards.buildingsmart.org/IDS/ids.xsd" xmlns:ids="http://standards.buildingsmart.org/IDS">
  <ids:info>
    <ids:title />
  </ids:info>
  <ids:specifications>
    <ids:specification ifcVersion="IFC4" name="There should be a Project Name" minOccurs="1">
      <ids:applicability>
        <ids:entity>
          <ids:name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="IfcProject" />
            </xs:restriction>
          </ids:name>
          <ids:predefinedType>
            <ids:simpleValue></ids:simpleValue>
          </ids:predefinedType>
        </ids:entity>
      </ids:applicability>
      <ids:requirements>
        <ids:attribute minOccurs="1" maxOccurs="unbounded">
          <ids:name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Name" />
            </xs:restriction>
          </ids:name>
          <ids:value>
            <xs:restriction base="xs:string">
              <xs:minLength value="1" />
              <xs:maxLength value="20" />
            </xs:restriction>
          </ids:value>
        </ids:attribute>
      </ids:requirements>
    </ids:specification>
    <ids:specification ifcVersion="IFC4" name="There should be a Site Name" minOccurs="1">
      <ids:applicability>
        <ids:entity>
          <ids:name>
            <ids:simpleValue>IfcSite</ids:simpleValue>
          </ids:name>
          <ids:predefinedType>
            <ids:simpleValue></ids:simpleValue>
          </ids:predefinedType>
        </ids:entity>
      </ids:applicability>
      <ids:requirements>
        <ids:attribute minOccurs="1" maxOccurs="unbounded">
          <ids:name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Name" />
            </xs:restriction>
          </ids:name>
          <ids:value>
            <xs:restriction base="xs:string">
              <xs:minLength value="1" />
              <xs:maxLength value="20" />
            </xs:restriction>
          </ids:value>
        </ids:attribute>
      </ids:requirements>
    </ids:specification>
    <ids:specification ifcVersion="IFC4" name="Are Levels Defined" minOccurs="1">
      <ids:applicability>
        <ids:entity>
          <ids:name>
            <ids:simpleValue>IfcBuildingStorey</ids:simpleValue>
          </ids:name>
        </ids:entity>
      </ids:applicability>
      <ids:requirements>
        <ids:entity>
          <ids:name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="IFCBUILDINGSTOREY" />
            </xs:restriction>
          </ids:name>
          <ids:predefinedType>
            <ids:simpleValue></ids:simpleValue>
          </ids:predefinedType>
        </ids:entity>
        <ids:attribute minOccurs="1" maxOccurs="unbounded">
          <ids:name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Name" />
            </xs:restriction>
          </ids:name>
          <ids:value>
            <xs:restriction base="xs:string">
              <xs:pattern value="[a-zA-Z0-9_ ]*" />
            </xs:restriction>
          </ids:value>
        </ids:attribute>
      </ids:requirements>
    </ids:specification>
    <ids:specification ifcVersion="IFC4" name="A Uniclass Classification System Exists" minOccurs="1">
      <ids:applicability>
        <ids:entity>
          <ids:name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="IfcClassification" />
            </xs:restriction>
          </ids:name>
        </ids:entity>
      </ids:applicability>
      <ids:requirements>
        <ids:attribute minOccurs="1" maxOccurs="unbounded">
          <ids:name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Name" />
            </xs:restriction>
          </ids:name>
          <ids:value>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Uniclass" />
            </xs:restriction>
          </ids:value>
        </ids:attribute>
      </ids:requirements>
    </ids:specification>
    <ids:specification ifcVersion="IFC4" name="Spaces are Defined" minOccurs="1">
      <ids:applicability>
        <ids:entity>
          <ids:name>
            <ids:simpleValue>IfcSpace</ids:simpleValue>
          </ids:name>
          <ids:predefinedType>
            <ids:simpleValue></ids:simpleValue>
          </ids:predefinedType>
        </ids:entity>
      </ids:applicability>
      <ids:requirements>
        <ids:entity>
          <ids:name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="IFCSPACE" />
            </xs:restriction>
          </ids:name>
        </ids:entity>
        <ids:attribute minOccurs="1" maxOccurs="unbounded">
          <ids:name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Name" />
            </xs:restriction>
          </ids:name>
          <ids:value>
            <xs:restriction base="xs:string">
              <xs:pattern value="\w*" />
            </xs:restriction>
          </ids:value>
        </ids:attribute>
      </ids:requirements>
    </ids:specification>
  </ids:specifications>
</ids:ids>