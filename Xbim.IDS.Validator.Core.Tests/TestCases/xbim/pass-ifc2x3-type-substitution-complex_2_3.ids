<?xml version="1.0" encoding="utf-8"?>
<ids xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:schemaLocation="http://standards.buildingsmart.org/IDS http://standards.buildingsmart.org/IDS/1.0/ids.xsd" xmlns="http://standards.buildingsmart.org/IDS" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <info>
    <title>Utilising Multiple IFC4 entity types agains IFC2X3 models</title>
  </info>
  <specifications>
    <specification name="We can locate IFCDUCTSEGMENTS and IFCDUCTFITTINGs on IFC2x3" ifcVersion="IFC2X3">
      <applicability minOccurs="1" maxOccurs="unbounded">
        <entity>
          <name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="IFCDUCTFITTING" />
              <xs:enumeration value="IFCDUCTSEGMENT" />
            </xs:restriction>
          </name>
        </entity>
      </applicability>
      <requirements>
        <attribute>
          <name>
            <simpleValue>Name</simpleValue>
          </name>
          <value>
            <simpleValue>DuctItem</simpleValue>
          </value>
        </attribute>
      </requirements>
    </specification>
  </specifications>
</ids>