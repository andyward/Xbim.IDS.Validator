<ids xmlns="http://standards.buildingsmart.org/IDS" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://standards.buildingsmart.org/IDS/ids_05.xsd">
  <info>
        <title>Components have valid installation date</title>
    </info>
    <specifications>
        <specification name="All components should have a InstallationDate after Dec 2023" ifcVersion="COBie2.4" minOccurs="1" maxOccurs="unbounded">
            <applicability>
                <entity>
                    <name>
                        <simpleValue>COBIECOMPONENT</simpleValue>
                    </name>
                </entity>
            </applicability>
            <requirements>
              <attribute minOccurs="1" maxOccurs="1">
                <name>
                  <simpleValue>InstallationDate</simpleValue>
                </name>
                <value>
                  <xs:restriction base="xs:string">
                    <xs:minExclusive value="2023-12-01" fixed="false" />
                  </xs:restriction>
                </value>
              </attribute>
            </requirements>
        </specification>
    </specifications>
</ids>